    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nestjs-command": "^3.1.4",
    "nestjs-i18n": "^10.2.6",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.1",
