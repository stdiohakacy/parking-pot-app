    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/typeorm": "^10.0.0",
    "@types/multer": "^1.4.7",
    "bcryptjs": "^2.4.3",
